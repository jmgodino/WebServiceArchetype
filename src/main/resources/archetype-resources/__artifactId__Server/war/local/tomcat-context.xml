#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<Context>
    <Resource name="${databaseJndi}"
        url="${databaseUrl}"
        driverClassName="org.h2.Driver" username="${databaseUser}"
        password="${databasePasswd}" auth="Container" type="javax.sql.DataSource"
        maxActive="3" maxIdle="2" maxWait="10000" />
</Context>
